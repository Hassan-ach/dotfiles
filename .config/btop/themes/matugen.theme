# Main background, empty for terminal default, need to be empty if you want transparent background
theme[main_bg]=""

# Main text color
theme[main_fg]="#e1e2e8"

# Title color for boxes
theme[title]="#a1c9fd"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#bbc7db"

# Background color of selected item in processes box
theme[selected_bg]="#a1c9fd"

# Foreground color of selected item in processes box
theme[selected_fg]="#003259"

# Color of inactive/disabled text
theme[inactive_fg]="#c3c6cf"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#d7bde4"

# Cpu box outline color
theme[cpu_box]="#8d9199"

# Memory/disks box outline color
theme[mem_box]="#8d9199"

# Net up/down box outline color
theme[net_box]="#8d9199"

# Processes box outline color
theme[proc_box]="#8d9199"

# Box divider line and small boxes line color
theme[div_line]="#43474e"

# Temperature graph colors
theme[temp_start]="#bbc7db"
theme[temp_mid]="#a1c9fd"
theme[temp_end]="#ffb4ab"

# CPU graph colors
theme[cpu_start]="#bbc7db"
theme[cpu_mid]="#a1c9fd"
theme[cpu_end]="#ffb4ab"

# Mem/Disk free meter
theme[free_start]="#bbc7db"
theme[free_mid]=""
theme[free_end]="#3c4858"

# Mem/Disk cached meter
theme[cached_start]="#d7bde4"
theme[cached_mid]=""
theme[cached_end]="#533f5f"

# Mem/Disk available meter
theme[available_start]="#a1c9fd"
theme[available_mid]=""
theme[available_end]="#1b4975"

# Mem/Disk used meter
theme[used_start]="#ffb4ab"
theme[used_mid]=""
theme[used_end]="#93000a"

# Download graph colors
theme[download_start]="#bbc7db"
theme[download_mid]="#a1c9fd"
theme[download_end]="#d7bde4"

# Upload graph colors
theme[upload_start]="#bbc7db"
theme[upload_mid]="#a1c9fd"
theme[upload_end]="#d7bde4"
